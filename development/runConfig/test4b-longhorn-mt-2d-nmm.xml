<?xml version="1.0"?>
<MagnetosphereTopologyBatch>

  <vtkSQBOVReader
      decomp_dims="5 10 5"
      block_cache_size="250"
      meta_read="1">
       <arrays> b </arrays>
  </vtkSQBOVReader>

  <vtkSQHemisphereSource
      north="0 1 0"
      center="255.5 511.5 255.5"
      radius="50"
      resolution="32"/>

  <vtkSQPlaneSource
      origin="150 445 256.5"
      point1="190 445 256.5"
      point2="150 570 256.6"
      resolution="400 1250"/>

  <vtkSQFieldTracer
      mode="2"
      integrator_type="3"
      min_step_size="1e-8"
      max_step_size="0.1"
      max_error="1e-3"
      max_number_steps="10000"
      max_arc_length="10000"
      null_threshold="1e-3"
      dynamic_scheduler="1"
      master_block_size="16"
      worker_block_size="512"/>

</MagnetosphereTopologyBatch>
