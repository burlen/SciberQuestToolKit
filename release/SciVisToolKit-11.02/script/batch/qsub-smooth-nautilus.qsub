#PBS -j eo

date
echo "Starting ParaView..."
echo "MPI_TYPE_MAX=$MPI_TYPE_MAX"
echo "LD_LIBRARY_PATH=$LD_LIBRARY_PATH"
echo "PATH=$PATH"
echo "PV_PATH=$PV_PATH"
echo "PV_NCPUS=$PV_NCPUS"
echo "SMB_EXE=$SMB_EXE"
