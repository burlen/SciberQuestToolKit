<?xml version="1.0"?>
<VortexDetectBatch>

  <vtkSQBOVReader
      cb_enable="1"
      ISubset="-1 -1"
      JSubset="-1 -1"
      KSubset="-1 -1">
       <arrays> b ui ue </arrays>
  </vtkSQBOVReader>

  <vtkSQVortexFilter computeRotation="1" computeDivergence="1" computeNormalizedHelicity="1"/>

  <vtkSQBOVWriter
      cb_enable="1"
      cb_buffer_size="1073741824"
      stripe_count="160"/>

</VortexDetectBatch>

