<?xml version="1.0"?>
<VortexDetectBatch>

  <vtkSQBOVReader
      cb_enable="0"
      ISubset="-1 -1"
      JSubset="-1 -1"
      KSubset="-1 -1">
       <arrays> B Ui Ue J </arrays>
  </vtkSQBOVReader>

  <vtkSQVortexFilter
      splitComponents="0"
      resultMagnitude="1"
      computeRotation="1"
      computeGradient="1"/>

  <vtkSQBOVWriter
      direct_io="0"
      cb_enable="0"
      cb_buffer_size="0"
      stripe_count="0"/>

</VortexDetectBatch>

