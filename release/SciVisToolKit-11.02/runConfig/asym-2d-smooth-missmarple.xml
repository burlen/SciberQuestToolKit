      stencilWidth="19"
      CPUDriverOptimization="1"
      numberOfMPIRanksToUseCUDA="0"
      numberOfActiveCUDADevices="0"
      numberOfWarpsPerCUDABlock="8"
      inputCUDAMemoryType="2"
      kernelCUDAMemoryType="0"/>
  -->

  <vtkSQBOVWriter
      cb_enable="1"
      cb_buffer_size="1073741824"
      stripe_count="160"/>

</SmoothBatch>

