      cb_enable="1"
      ISubset="-1 -1"
      JSubset="-1 -1"
      KSubset="-1 -1">
       <arrays> b </arrays>
  </vtkSQBOVReader>

  <vtkSQMedianFilter stencilWidth="9" />

  <!--
  memory types:
    gloabl=0
    constant=1
    texture=2
